





#include <iostream>
#include <cstdio>
using namespace std;


int max_of_four(int a, int b, int c, int d){
    
    int max;
    
    int arr[4]={a,b,c,d};
    
    for(int i=0;i<4;i++){
        if(max < arr[i]){
            max = arr[i];
        }
    }
    
    return max;
}


int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans;
    ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}


